{
  "name": "@omnirealm/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && node scripts/generate-sitemap.js",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write 'src/**/*.{tsx,ts,jsx,js}'",
    "validate": "pnpm run lint && pnpm run test",
    "validate:quick": "pnpm run lint",
    "journal": "node ../../../shared/scripts/journal-unified.cjs add",
    "journal:search": "node ../../../shared/scripts/journal-unified.cjs search",
    "journal:stats": "node ../../../shared/scripts/journal-unified.cjs stats",
    "context:save": "node ../../../shared/scripts/journal-unified.cjs context save",
    "context:load": "node ../../../shared/scripts/journal-unified.cjs context load",
    "ab:enable": "node scripts/toggle-ab-testing.js enable",
    "ab:disable": "node scripts/toggle-ab-testing.js disable",
    "ab:status": "node scripts/toggle-ab-testing.js status"
  },
  "dependencies": {
    "next": "^14.2.30",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.15.0",
    "js-cookie": "^3.0.5",
    "posthog-js": "^1.249.0",
    "tailwind-merge": "^2.5.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@types/testing-library__react": "^10.0.1",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^30.0.0-beta.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.3.3",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.0.11",
    "jest": "^30.0.0-beta.3",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "lint-staged": "^16.1.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.2",
    "typescript": "^5.7.3"
  }
}
