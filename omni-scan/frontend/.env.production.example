# OmniScan Frontend - Production Environment Variables
# Copy this file to .env.production and fill with your values

# Supabase Configuration (Public keys only!)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# API Configuration
VITE_API_URL=https://api.scan.omnirealm.tech
VITE_API_VERSION=v1

# App Configuration
VITE_APP_NAME=OmniScan
VITE_APP_VERSION=2.0.0

# Features Flags
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_BATCH_UPLOAD=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_CACHE=true

# Upload Configuration
VITE_MAX_FILE_SIZE=52428800  # 50MB in bytes
VITE_ALLOWED_FILE_TYPES=.pdf,.png,.jpg,.jpeg,.gif,.bmp,.tiff,.txt,.doc,.docx

# Analytics (optional)
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_POSTHOG_KEY=phc_your-posthog-key

# Sentry (optional)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Public URLs
VITE_PUBLIC_URL=https://scan.omnirealm.tech
VITE_SUPPORT_EMAIL=support@omnirealm.tech