{
  "name": "@omnirealm/omni-fit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:fast": "vite --force --clearScreen false",
    "dev:direct": "node node_modules/vite/bin/vite.js --host",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "build:production": "./scripts/build-production.sh",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "test": "node --experimental-vm-modules node_modules/.bin/vitest",
    "test:simple": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:mem": "node --max-old-space-size=4096 node_modules/.bin/vitest",
    "test:single": "vitest --run --reporter=verbose",
    "test:fix": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vitest run",
    "test:optimized": "./run-tests-optimized.sh",
    "test:analytics": "vitest run src/__tests__/services/analytics.optimized.test.ts",
    "test:quick": "vitest run --bail=1 --reporter=basic --exclude='**/backup.test.ts'",
    "deploy:vercel": "./scripts/deploy-vercel.sh",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "deploy:github": "pnpm run build && gh-pages -d dist",
    "predeploy": "pnpm run build",
    "analyze": "npx vite-bundle-analyzer dist/",
    "cap:sync": "npm run build && npx cap sync",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios",
    "cap:run:android": "npm run cap:sync && npx cap run android",
    "cap:run:ios": "npm run cap:sync && npx cap run ios",
    "journal": "node ../../../shared/scripts/journal-unified.cjs add",
    "journal:search": "node ../../../shared/scripts/journal-unified.cjs search",
    "journal:stats": "node ../../../shared/scripts/journal-unified.cjs stats",
    "context:save": "node ../../../shared/scripts/journal-unified.cjs context save",
    "context:load": "node ../../../shared/scripts/journal-unified.cjs context load",
    "test:ci": "node --experimental-vm-modules node_modules/.bin/vitest"
  },
  "dependencies": {
    "@omnirealm/auth": "workspace:*",
    "@omnirealm/compliance": "workspace:*",
    "@omnirealm/supabase-kit": "workspace:*",
    "@omnirealm/ui": "workspace:*",
    "@omnirealm/utils": "workspace:*",
    "@stripe/stripe-js": "^7.6.1",
    "chart.js": "^4.4.2",
    "date-fns": "^3.3.1",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.7",
    "framer-motion": "^11.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "stripe": "^18.3.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fake-indexeddb": "^6.0.1",
    "gh-pages": "^6.3.0",
    "jsdom": "^24.0.0",
    "vercel": "^44.5.3",
    "vite-plugin-pwa": "^1.0.1",
    "workbox-window": "^7.0.0"
  },
  "description": "Coach fitness IA premium avec suivi personnalis√© des performances.",
  "main": "postcss.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
